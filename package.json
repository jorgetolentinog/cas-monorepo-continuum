{
  "name": "cas-continuum",
  "version": "0.0.0",
  "scripts": {
    "@cas-medicap-replicator/message:start": "turbo run start --filter=@cas-medicap-replicator/message",
    "@cas-medicap-replicator/message2:start": "turbo run start --filter=@cas-medicap-replicator/message2",
    "telemedicine-schedule-availability:start": "turbo run start --filter=telemedicine-schedule-availability",
    "deploy": "npm run deploy:resource && npm run deploy:module",
    "deploy:resource": "turbo run deploy --filter=*-resource",
    "deploy:module": "turbo run deploy --filter=!*-resource",
    "remove": "turbo run remove"
  },
  "workspaces": [
    "@package/*",
    "@cas-medicap-replicator/*",
    "@cas-telemedicine-schedule/*"
  ],
  "devDependencies": {
    "turbo": "^1.6.3"
  }
}
