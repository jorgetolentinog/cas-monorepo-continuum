{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2019",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
